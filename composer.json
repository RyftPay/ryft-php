{
    "name": "ryftpay/ryft-php",
    "description": "RyftPay SDK for PHP",
    "homepage": "https://github.com/ryftpay/ryft-php",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "ryft",
        "ryftpay",
        "payment",
        "SDK",
        "Library",
        "PHP",
        "API"
    ],
    "authors": [
        {
            "name": "RyftPay",
            "homepage": "https://github.com/ryftpay/ryft-php/graphs/contributors",
            "email": "tech@ryftpay.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^9.0",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Ryft\\": "lib/Ryft"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ryft\\Tests\\": "tests/Ryft"
        }
    },
    "scripts": {}
}
